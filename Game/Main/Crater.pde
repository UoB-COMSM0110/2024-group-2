public class Crater{
  
  private float x, y;
  private int size;
  
  public Crater(float x, float y, int size){
    this.x = x;
    this.y = y;
    this.size = size;
  }
  
  public float getX(){
    return this.x;
  }
  
  public float getY(){
    return this.y;
  }
  
  public int getSize(){
    return this.size;
  }
}
